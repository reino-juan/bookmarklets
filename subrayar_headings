javascript:void function(){  var t = document.createElement("style");  var e = document.createTextNode('h1 { outline: 1px dashed black !important; background-color: lightgreen !important; }h2 { outline: 1px dashed black !important; background-color: %23ffc966 !important; }h3 { outline: 1px dashed black !important; background-color: lightblue !important; }h4, h5, h6 { outline: 1px dashed black !important; background-color: %23ddd !important; }h1:before, h2:before, h3:before, h4:before, h5:before, h6:before { font-size: 12px !important; margin-right: 4px !important;}h1:before { content: " [H1]" !important;}h2:before { content: " [H2]" !important;}h3:before { content: " [H3]" !important;}h4:before { content: " [H4]" !important;}h5:before { content: " [H5]" !important;}h6:before { content: " [H6]" !important;}');  t.appendChild(e);  var removeButton = document.createElement("button");  var buttonText = document.createTextNode("Remove Styles");  removeButton.appendChild(buttonText);  removeButton.style.position = "fixed";  removeButton.style.top = "10px";  removeButton.style.left = "10px";  removeButton.style.padding = "8px 12px";  removeButton.style.backgroundColor = "red";  removeButton.style.color = "white";  removeButton.style.border = "none";  removeButton.style.borderRadius = "4px";  removeButton.style.fontFamily = "Arial, sans-serif";  removeButton.onclick = function() {    t.remove();    removeButton.remove();  };  var o = document.getElementsByTagName("head");  o[0].appendChild(t);  document.body.appendChild(removeButton);}();
